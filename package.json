    "react-native-numeric-input": "^1.8.3",
    "react-native-orientation-loading-overlay": "^0.1.6",
    "react-native-phone-input": "^0.2.4",
    "react-native-picker-checkbox": "^2.0.1",
    "react-native-pixel-perfect": "^1.0.2",
    "react-native-select-multiple": "^2.0.0",
    "react-native-simple-radio-button": "^2.7.4",
    "react-native-svg": "^7.0.3",
    "react-native-textarea": "^1.0.3",
    "react-native-timer": "^1.3.6",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^1.5.8",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.2.0",
    "rn-fetch-blob": "^0.10.14"
  },
  "devDependencies": {
    "@svgr/cli": "^3.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "23.6.0",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-native": "^3.2.1",
    "husky": "^0.14.3",
    "jest": "23.6.0",
